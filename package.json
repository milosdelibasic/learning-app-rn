{
  "name": "PhantoxLearn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android",
    "android:release": "ENVFILE=.env.development cd android && ./gradlew clean && cd .. && react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --variant=release",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "linux": "ENVFILE=.env.development react-native run-android && react-native start",
    "linuxStaging": "ENVFILE=.env.staging  react-native run-android && react-native start",
    "clean": "cd android && ./gradlew clean && cd ..",
    "cleanRun": "cd android && ./gradlew clean && cd .. && npm run linux",
    "cleanCache": "npm start -- --reset-cache"
  },
